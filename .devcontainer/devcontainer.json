{
  "name": "Zettel",
  "remoteUser": "vscode",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "node": "20.7",
    "rust": "1.72"
  },
  "containerEnv": {
    "WEBKIT_DISABLE_COMPOSITING_MODE": "1",
},
"customizations": {
	"vscode": {
		"extensions": [
			"svelte.svelte-vscode",
			"rust-lang.rust-analyzer",
			"dbaeumer.vscode-eslint",
			"bradlc.vscode-tailwindcss",
			"nrwl.angular-console",
			"esbenp.prettier-vscode"
		]
	}
}
}
